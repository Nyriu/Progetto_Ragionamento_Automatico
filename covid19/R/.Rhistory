tabelFromFiles = function (files, convertTimeout=F) {
data =
files %>%
map_df(
function(t)
read_json(t) %>%
spread_all() %>%
as_tibble() %>%
select(-document.id)
) %>%
#select(-sol) %>%
mutate(file_path=unlist(files)) %>%
mutate(num = row_number() - 1) %>%
select(num, file_path, everything())
